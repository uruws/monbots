// check api user profile
USER_URL = config.get("USER_URL")
api_login()
resp = bot.get(USER_URL)
assert.http_status(resp, http.status_ok)
assert.http_header(resp, "content-type", "application/json")
assert.json_match(resp, "response status",
	`{"status": "success"}`)
assert.json_match(resp, "user id",
	`{"data": {"user": {"_id": "8dTvvdraTKNDzZ4u5"}}}`)
assert.json_match(resp, "user fullname",
	`{"data": {"user": {"firstName": "Albert", "lastName": "Einstein"}}}`)
api_logout()
